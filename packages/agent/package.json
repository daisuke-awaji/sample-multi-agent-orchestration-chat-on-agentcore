{
  "name": "@fullstack-agentcore/agent",
  "version": "0.1.0",
  "description": "Strands AI Agent for Amazon Bedrock AgentCore Runtime",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test:local": "tsx src/test.ts",
    "clean": "rimraf dist",
    "docker:build": "docker build -t agentcore-agent .",
    "docker:run": "docker run -p 8080:8080 agentcore-agent",
    "docker:dev": "docker-compose up --build",
    "docker:dev:detach": "docker-compose up -d --build",
    "docker:dev:aws": "./scripts/get-aws-credentials.sh",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:test": "docker-compose exec agentcore-agent curl http://localhost:8080/ping"
  },
  "keywords": [
    "strands",
    "agentcore",
    "ai",
    "bedrock",
    "runtime"
  ],
  "author": "AgentCore Runtime Agent",
  "license": "MIT",
  "dependencies": {
    "@strands-agents/sdk": "^0.1.3",
    "express": "^4.18.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
