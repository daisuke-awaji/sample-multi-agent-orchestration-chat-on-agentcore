{
  "name": "@fullstack-agentcore/agent",
  "version": "0.1.0",
  "description": "Strands AI Agent for Amazon Bedrock AgentCore Runtime",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --passWithNoTests",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --coverage",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --testMatch='**/*.integration.test.ts' --testPathIgnorePatterns='/node_modules/'",
    "test:local": "tsx src/test.ts",
    "clean": "rimraf dist",
    "docker:build": "docker build -t agentcore-agent .",
    "docker:run": "docker run -p 8080:8080 agentcore-agent",
    "docker:dev": "docker-compose up --build",
    "docker:dev:detach": "docker-compose up -d --build",
    "docker:dev:aws": "./scripts/get-aws-credentials.sh",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:test": "docker-compose exec agentcore-agent curl http://localhost:8080/ping"
  },
  "keywords": [
    "strands",
    "agentcore",
    "ai",
    "bedrock",
    "runtime"
  ],
  "author": "AgentCore Runtime Agent",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-agentcore": "^3.956.0",
    "@aws-sdk/client-bedrock-agentcore-control": "^3.958.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.654.0",
    "@aws-sdk/client-dynamodb": "^3.654.0",
    "@aws-sdk/client-s3": "^3.654.0",
    "@aws-sdk/client-secrets-manager": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.654.0",
    "@aws-sdk/util-dynamodb": "^3.654.0",
    "@fullstack-agentcore/tool-definitions": "*",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@strands-agents/sdk": "^0.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ignore": "^7.0.5",
    "nanoid": "^5.1.5",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "jest": "^30.2.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.4.6",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
