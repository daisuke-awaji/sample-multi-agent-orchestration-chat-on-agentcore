{
  "name": "@moca/trigger",
  "version": "0.1.0",
  "description": "Event-driven trigger Lambda for AgentCore invocations",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --passWithNoTests --forceExit",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest integration.test.ts --forceExit",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --coverage",
    "clean": "rimraf dist"
  },
  "keywords": [
    "agentcore",
    "trigger",
    "scheduler",
    "eventbridge"
  ],
  "author": "AgentCore Trigger",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.654.0",
    "@aws-sdk/client-dynamodb": "^3.654.0",
    "@aws-sdk/util-dynamodb": "^3.654.0",
    "nanoid": "^5.1.5"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "dotenv": "^17.2.3",
    "jest": "^30.2.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.4.6",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
