{
  "name": "@moca/session-stream-handler",
  "version": "0.1.0",
  "description": "DynamoDB Streams handler for session events, publishes to AppSync Events",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --passWithNoTests --forceExit",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --coverage",
    "clean": "rimraf dist"
  },
  "keywords": [
    "agentcore",
    "dynamodb-streams",
    "appsync-events",
    "realtime"
  ],
  "author": "AgentCore Session Stream Handler",
  "license": "MIT",
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/credential-provider-node": "^3.750.0",
    "@smithy/signature-v4": "^4.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "jest": "^30.2.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
